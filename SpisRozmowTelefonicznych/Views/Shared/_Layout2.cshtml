<!DOCTYPE HTML>

<html lang="pl">

<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="description" content="Serwis ten pomoze Ci spis wszystkie polaczenie Twojej firmy" />
    <meta name="keywords" content="polaczenia,spis,telefon,firma,baza" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    @*<link rel="stylesheet" href="~/Content/Formularz.css" type="text/css" />*@
    
    <link href="@Url.Content("~/Content/CallStyle.css")" rel="stylesheet" />
    @System.Web.Optimization.Styles.Render("~/Content/themes/base/css")
</head>
<body>

    <div id="container">

         
        <div id="logo">
            <h1>Spis polaczen telefonicznych</h1>

           
        </div>
         

        <div id="nav">
            @Html.Partial("_CallsMenu")
           
        </div>

        <div id="content">
            
            @RenderBody()









        </div>

        <div id="ad">
        @Html.Partial("_LoginPartial")  
        </div>

        <div id="footer">
            Spis Polaczen Telefonicznych &copy; Wszelkie prawa zastrzeżone
        </div>
    </div>


@RenderSection("Scripts", required: false)
</body>
</html> 